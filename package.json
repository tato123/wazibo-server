{
  "name": "wazibo-server",
  "version": "1.0.0",
  "description": "Wazibo backend api",
  "main": "index.js",
  "scripts": {
    "lint": "jshint --exclude-path=.gitignore . || true",
    "test": "npm run lint",
    "start": "npm run apidoc && nodemon ${SCRIPT:-index.js}",
    "db": "rm -fr .mongodb && mkdir .mongodb && mongod --dbpath .mongodb",
    "docker-build": "docker build -t us.gcr.io/wazibo-1143/wazibo-server .",
    "docker-deploy": "gcloud docker push us.gcr.io/wazibo-1143/wazibo-server",
    "gcloud-deploy": "npm run docker-build && npm run docker-deploy",
    "apidoc": "apidoc -f \".*\\.js$\" -f \".*\\.api$\" -i routes/ -o public/api/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tato123/wazibo-server.git"
  },
  "private": true,
  "author": "Wazibo",
  "contributors": [
    {
      "name": "Jonathan Fontanez",
      "email": "fontanezj1@gmail.com"
    }
  ],
  "license": "Wazibo",
  "dependencies": {
    "body-parser": "^1.13.1",
    "compression": "^1.5.0",
    "express": "^4.13.3",
    "express-load-routes": "^1.0.1",
    "express-session": "^1.12.1",
    "gcloud": "^0.25.1",
    "googleapis": "^2.1.6",
    "kerberos": "0.0.17",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "mongodb": "^2.0.52",
    "mongoose": "^4.2.8",
    "mongoose-timestamp": "^0.4.0",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "passport-facebook-token": "tato123/passport-facebook-token",
    "q": "^1.4.1",
    "winston": "^2.1.1"
  },
  "engines": {
    "node": "0.12.x",
    "npm": ">2.11.3"
  }
}
