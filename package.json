{
  "name": "wazibo-server",
  "version": "1.0.0",
  "description": "Wazibo backend api",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint --exclude-path=.gitignore . || true",
    "test": "npm run lint",
    "start": "npm run apidoc && node ${SCRIPT:-index.js}",
    "db": "mongod --dbpath .mongodb",
    "docker-build": "docker build -t gcr.io/wazibo-1143/wazibo-server .",
    "docker-deploy": "gcloud docker push gcr.io/wazibo-1143/wazibo-server",
    "gcloud-deploy": "npm run docker-build && npm run docker-deploy",
    "apidoc" : "./node_modules/apidoc/bin/apidoc -i routes/ -o public/api/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tato123/wazibo-server.git"
  },
  "private": true,
  "author": "Wazibo",
  "contributors": [
    {
      "name": "Jonathan Fontanez",
      "email": "fontanezj1@gmail.com"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.13.1",
    "compression": "^1.5.0",
    "express": "^4.13.3",
    "express-load-routes": "^1.0.1",
    "express-session": "^1.12.1",
    "gcloud": "^0.25.1",
    "googleapis": "^2.1.6",
    "lodash": "^3.10.1",
    "mongoose": "^4.2.8",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "q": "^1.4.1"
  },
  "engines": {
    "node": "0.10.x",
    "npm": ">1.4.0"
  },
  "devDependencies": {
    "apidoc": "^0.13.1",
    "jshint": "^2.9.1-rc1"
  }
}
